{
  "name": "screener-storybook",
  "version": "1.0.1",
  "description": "Automated Visual Testing for React Storybook using Screener.io",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/screener-io/screener-storybook.git"
  },
  "bin": {
    "screener-storybook": "./bin/screener-storybook.js"
  },
  "scripts": {
    "test": "npm run lint && nyc mocha \"test/unit/*.spec.js\" && npm run coverage",
    "lint": "eslint .",
    "coverage": "nyc check-coverage --statements 90 --functions 90 --branches 80 --lines 90"
  },
  "nyc": {
    "include": [
      "src/check.js",
      "src/scripts/story-steps.js",
      "src/runner.js",
      "src/validate.js"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "peerDependencies": {
    "storybook": ">= 2"
  },
  "dependencies": {
    "@types/react": "*",
    "bluebird": "~3.7.2",
    "colors": "~1.1.2",
    "commander": "~6.2.0",
    "express": "^4.17.1",
    "get-port": "~4.2.0",
    "joi": "~14.3.1",
    "js-yaml": "^3.13.1",
    "lodash": "~4.17.21",
    "prop-types": "^15.6.0",
    "puppeteer": "^18.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "request": "^2.87.0",
    "requestretry": "7.0.0",
    "screener-runner": "^0.14.0",
    "semver": "~5.6.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "^8.0.0",
    "mocha": "^10.0.0",
    "nyc": "^14.1.1",
    "rewire": "^6.0.0"
  }
}
